{
	"name": "CÃ¡lculo Lambda",
	"scopeName": "source.lambda",
	"patterns": [
		{
			"name": "variable.parameter.lambda",
			"match": "\\\\(\\w+\\s?)+."
		},
		{
			"name": "keyword.control.lambda",
			"match": "\\b\\w+\\s*="
		},
		{
			"name": "constant.numeric.lambda",
			"match": "\\b\\d+\\b"
		},
		{
			"name": "comment.line.double-dash.lambda",
			"match": "--.*$"
		},
		{
			"name": "application.lambda",
			"match": "\\b\\w+\\s\\w+\\b"
		}
	]
}